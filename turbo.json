{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["NEXTAUTH_URL", "MONGODB_URI", "NEXTAUTH_SECRET"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": ["NEXTAUTH_URL"],
      "outputs": ["dist/**"]
    }
  },
  "globalEnv": [],
  "globalDependencies": ["tsconfig.json"]
}
